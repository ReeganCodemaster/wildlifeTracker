<%= form_with model: animal do |form| %>
    <div>
        <%= form.label :animal %><br/>
        <%= form.text_field :animal %><br/>
        <ul>
            <%animal.errors.full_messages_for(:animal).each do %>
                <li>
                    <%= message%>
                </li>
            <% end %>
        </ul>
    </div>
    <div>
        <%= form.submit%>
    </div>
<% end %>